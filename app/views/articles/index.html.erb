<h2>Index of Articles</h2>
<hr>

<table>
    <thead>
        <tr>
            <th>id</th>
            <th>Title</th>
            <th>Added at</th>
            <th>Action</th>
        </tr>
    </thead>


    <tbody>
        <% @articles.each do |article| %>
        <tr>
            <td><%= article.id %></td>
            <td><%= article.title %></td>
            <td><%= article.created_at %></td>
            <td>
                <button>View</button>
                <button>Edit</button>
                <button>Delete</button>
            </td>
        </tr>
        <% end %>
    </tbody>
</table>